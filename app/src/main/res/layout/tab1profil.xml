<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity$PlaceholderFragment">

    <TextView
        android:id="@+id/textView18"
        android:layout_width="105dp"
        android:layout_height="28dp"
        android:text="@string/email_text"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView15"
        tools:layout_editor_absoluteX="77dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="105dp"
        android:layout_height="28dp"
        android:text="@string/lastname_example"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView14"
        tools:layout_editor_absoluteX="77dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="105dp"
        android:layout_height="28dp"
        android:text="@string/rating_example"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView16"
        tools:layout_editor_absoluteX="77dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="105dp"
        android:layout_height="28dp"
        android:text="@string/points_example"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView17"
        tools:layout_editor_absoluteX="77dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="105dp"
        android:layout_height="26dp"
        android:text="@string/firstname_example"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView13"
        tools:layout_editor_absoluteX="77dp" />

    <TextView
        android:id="@+id/section_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"
        android:layout_marginBottom="@dimen/activity_vertical_margin"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="60dp"
        android:layout_height="28dp"
        android:text="@string/firstname_text"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="96dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="60dp"
        android:layout_height="28dp"
        android:text="@string/lastname_text"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="132dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="60dp"
        android:layout_height="28dp"
        android:text="@string/email_text"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="168dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="60dp"
        android:layout_height="28dp"
        android:text="@string/rating_text"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="204dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="60dp"
        android:layout_height="28dp"
        android:text="@string/points_text"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="240dp" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/my_objects_text"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="289dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="29dp"
        android:layout_height="26dp"
        android:src="@android:drawable/ic_menu_edit"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="282dp" />

</android.support.constraint.ConstraintLayout>